        $('header').addClass('fixed_header');
    }
    else {
        $('header').removeClass('fixed_header');
    }
	
});
	

	


	

	
	
/** SMOOTH **/	
$(document).on('click', 'a.smoothLink[href^="#"]', function (event) {
    event.preventDefault();
    $('html, body').animate({
        scrollTop: $($.attr(this, 'href')).offset().top + -120
    }, 1000);
});
	

	
	
	
